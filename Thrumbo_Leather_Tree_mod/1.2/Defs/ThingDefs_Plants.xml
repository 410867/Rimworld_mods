<?xml version="1.0" encoding="utf-8" ?>
<Defs>


<ThingDef ParentName="TreeBase">
    <defName>Plant_ThrumboTree</defName>
    <label>Thrumbo tree</label>
    <description>It's a tree found somewhere in Rimworld, and its bark is very similar to Thrumbo's leather. It takes a long time to grow up.</description>
    <graphicData>
      <texPath>Things/Plant/ThrumboLeathertreetwo</texPath>
      <graphicClass>Graphic_Single</graphicClass>
        <shadowData>
          <volume>(0.15, 0.3, 0.15)</volume>
          <offset>(0,0,-0.38)</offset>
        </shadowData>
    </graphicData>
    <selectable>true</selectable>
    <statBases>
	<MaxHitPoints>200</MaxHitPoints>
      <Beauty>10</Beauty>
      <Nutrition>1.5</Nutrition>
    </statBases>
		<plant>
		<neverBlightable>true</neverBlightable>
		<dieIfLeafless>false</dieIfLeafless>
			<growDays>55</growDays>
			<visualSizeRange>1.3~2.0</visualSizeRange>
			<sowMinSkill>12</sowMinSkill>
      <sowTags>
			<li>Ground</li>
			</sowTags>
      <sowResearchPrerequisites>
        <li>ThrumboTree</li>
      </sowResearchPrerequisites>
      <topWindExposure>0.15</topWindExposure>
      <mustBeWildToSow>false</mustBeWildToSow>
      <immatureGraphicPath>Things/Plant/ThrumboLeathertreeone</immatureGraphicPath>
      <harvestWork>2000</harvestWork>
		<harvestTag>Standard</harvestTag>
      <harvestYield>40</harvestYield>
      <harvestedThingDef>Leather_Thrumbo</harvestedThingDef>
      <harvestFailable>true</harvestFailable>
    </plant>
  </ThingDef>

</Defs>
